              <body>
              
               <div class="col-md-6 col-xl-5 mb-4">
              
                          <!--Card-->
                          <div class="card">
              
                            <!--Card content-->
                            <div class="card-body">
              
                              <!-- Form -->
                              <form name="">
                                  <h3 class="dark-grey-text text-center">
                                  <strong>Batch Information</strong>
                                </h3>
                                <hr>
 
                                

                                <div class="form-group">
                                  <label for="sel1"></label>
                                  <select class="form-control" name="courseid" id="courses" required>
                                    <option>Select Course Name</option>
                                    
                                  </select>
                                </div>
                                <div class="form-group" id="sel1" name="batchtime" id="batchtime" required>
                                  <label for="sel1">Batch Time</label>
                                    
                                        
                                      <div class="form-group">
                                 <label for="sel1"></label>
                                        <select class="form-control" name="fromtime" id="fromtime" required>
                                          <option>From</option>
                                          
                                        </select>
                                       </div>

                                       <div class="form-group">
                                          <label for="sel1"></label>
                                                 <select class="form-control" name="totime" id="totime" required>
                                                   <option>To</option>
                                                   
                                                 </select>
                                                </div>
                                  
                                
                                  
                                <div class="form-group">
                                    <label for="sel1"></label>
                                    <select class="form-control" id="batchdays" name="batchdays" required>
                                        <option>Batch Days</option>
                                      <option>TTS</option>
                                      <option>MWF</option>
                                      <option>REGULAR</option>
                                      
                                    </select>
                                  </div>

                                  <div class="md-form">
                                 
                                    <input type="date" name='batchdate'>
                                    <label for="form2">Batch Date</label>
                                  </div>

                                 
                                  <div class="md-form">
                                  
                                    <input type="text" name="batchname" id="batchname" required>
                                      <label>Batch Name</label>
                                    </select>
                                  </div>
                                  <div class="md-form">
                                 
                                    <input type="date" name='addedon'>
                                    <label for="form2">Added On</label>
                                  </div>
                        
                                  <div class="md-form">
                                    
                                    <input type="text" id="form8" name='addedby' class="md-textarea">
                                    <label for="form8">Added By</label>
                                  </div>
                                  <div class="md-form">
                                      
                                     <input type="date"  name='lastupdate' >
                                      <label for="form9">Last Update</label>
                                    </div>

                                     <div class="text-center">
                                        <button class="btn btn-black">Submit</button> 
                                        <div class="alert alert-success">  
                                          <label for="form12"style="">MESSAGE:<%=msg%></label> 
                                       </div>
                                      </div>
                              </form>
                              <!-- Form -->
                            </center>              
                            </div>
                           
                          </div>
