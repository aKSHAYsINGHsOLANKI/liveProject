</head>
<body>
<script>

$(document).ready(function() {
    $('#example').DataTable();
} );

</script>
<table id="example"  class="table table-striped table-bordered" style="width:100%">
        <thead >
            <tr>
                <th>SERIAL NO.</th>
                <th>TRANSACTION ID</th>
                <th>BATCH ID</th>
                <th>COURSE ID</th>
                <th>STUDENT NAME</th>
                <th>GENDER</th>
                <th>MOBILE NO</th>
                <th>COLLEGE NAME</th>
                <th>SEMESTER</th>
                <th>BRANCH</th>
                <th>COURSE NAME</th>
                <th>BATCH NAME</th>
                <th>CURRENT DATE</th>
                <th>STATUS</th>
                <th>PICTURE</th>
                
          <th>UPDATE</th>
            </tr>
        </thead>
        <tbody>
            
                <% 
          data.map(function(item,key){
            
           
           var d=new Date(item.currentdate)
           bd=d.getFullYear()+"/"+(d.getMonth()+1)+"/"+d.getDate();
           var g=new Date(item.updateon)
           bo=g.getFullYear()+"/"+(g.getMonth()+1)+"/"+g.getDate();
           
            
           %>
 
           <tr><td><%=(key+1)%></td><td><%=item.transactionid%></td><td><%=item.batchid%></td><td><%=item.courseid%></td><td><%=item.studentname%></td><td><%=item.gender%></td><td><%=item.mobileno%></td><td><%=item.collegename%></td><td><%=item.semester%></td><td><%=item.branch%></td><td><%=item.coursename%></td><td><%=item.batchname%></td><td><%=item.currentdate%></td><td><%=item.status%></td><td><img src='http://localhost:3000/images/<%=item.picture%>' width='35' height="35"></td><td><a href='http://localhost:3000/student/studentdisplaybyid?tid=<%=item.transactionid%>'>Edit/Delete</a></td></tr>
        <% });
        %>
  
  
            
  </tbody>    
  <tfoot>
      <tr>
          <th>SERIAL NO.</th>
          <th>TRANSACTION ID</th>
          <th>BATCH ID</th>
          <th>COURSE ID</th>
          <th>STUDENT NAME</th>
          <th>GENDER</th>
